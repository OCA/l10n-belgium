<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_codabox_fetch_feed" model="ir.cron">
            <field name="name">CodaBox Fetch Data</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="model_id" ref="model_codabox_feed" />
            <field name="state">code</field>
            <field name="code">model.cron_fetch_feed()</field>
            <field
                name="nextcall"
                eval="(DateTime.now()).strftime('%Y-%m-%d 06:33:47')"
            />
        </record>
    </data>
</odoo>
