<odoo noupdate="1">
    <record forcecreate="True" id="ir_cron_statbel_retrieval_job" model="ir.cron">
        <field name="name">Statbel retrieval</field>
        <field ref="model_l10n_be_statbel_index" name="model_id" />
        <field name="state">code</field>
        <field name="code">model.cron_retrieve()</field>
        <field eval="True" name="active" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
    </record>
</odoo>
